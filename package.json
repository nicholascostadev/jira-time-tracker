{
  "name": "jira-time-tracker",
  "version": "2.0.0",
  "description": "Beautiful CLI tool for tracking time and logging worklogs to Jira",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "jtt": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "start": "bun run src/index.ts",
