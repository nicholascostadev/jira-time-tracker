    "open": "^11.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@types/node": "^25.2.1",
    "bun-types": "^1.3.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
