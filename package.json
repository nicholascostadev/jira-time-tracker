{
  "name": "@nicholascostadev/jira-time-tracker",
  "version": "0.2.1",
  "description": "Beautiful CLI tool for tracking time and logging worklogs to Jira",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "jtt": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "start": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release:tag": "bun run scripts/create-release-tag.ts"
  },
  "keywords": [
    "jira",
    "time-tracker",
    "cli",
    "worklog",
    "opentui"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@opentui/core": "^0.1.77",
    "commander": "^14.0.3",
    "conf": "^15.1.0",
    "jira.js": "^5.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/bun": "^1.3.8",
    "@types/node": "^25.2.1",
    "bun-types": "^1.3.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
